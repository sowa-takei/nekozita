<div class="container">
  <h4>ジャンル編集</h4>
  <div class="row">
    <div class="col-10">
      <%= form_with model: @genre, url:admin_genre_path(@genre),local:true do |f| %>
      <table>
        <tr>
          <th>ジャンル名</th>
          <td><%= f.text_field :name %>
              <%= f.submit "変更を保存",class:"btn btn-success" %> </td>
        </tr>
      </table>
      <% end %>
    </div>
  </div>
</div>