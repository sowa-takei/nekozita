<div class="container">
  <div class="row">
    <div class="col-2">
      <table>
        <tr>
         <th>BRAND</th>
        </tr>
        <tr>
          <td>nekozita</td>
        </tr>
      </table>
    </div>
    <div class="col-10">

    　<h4>新着商品</h4>
    　<div class="row mx-auto mt-3">
    <% @items.last(4).each do |item| %>
     <div class="mx-auto">
       <%= link_to item_path do %>
      <%= attachment_image_tag item, :image, :fill,100,100 %></br>
       <div class="row mt-3">
        <%= item.name %></br>
       </div>
       <div class="row mt-1">
        ￥<%= item.price %>
       </div>
       <% end %>
     </div>
    <% end %>
    <%#= link_to "全ての商品を見る",items_path %>
   </div>
  </div>
 </div>
</div>